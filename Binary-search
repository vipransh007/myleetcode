class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {


        String w = " ";
        String w2 = " ";
        

        for ( int i = 0 ;i < word1.length; i++){
            w += word1[i];
        }
        
        for ( int j = 0 ;j < word2.length; j++){
            w2 += word2[j];
        }

       return w.toString().equals(w2.toString());
    }
}
