import java.util.Arrays;

class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length;
      int sum = 0 ;
     int totalsum = 0;
     for(int i = 0; i< n ; i++){
         sum += nums[i];
     }
       totalsum = (n * (n+1) /2);
     int missing = totalsum - sum;
     return missing;
     
    }

}
